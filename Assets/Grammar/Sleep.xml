<?xml version="1.0" encoding="utf-8" ?>
<grammar
  version="1.0"
  mode="voice"
  xml:lang="en-US"
  tag-format="semantics/1.0"
  xmlns="http://www.w3.org/2001/06/grammar"
  root="sleep">

